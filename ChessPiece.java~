/**
 * @author Martin Guevara
 * @version January 14th, 2019
 */
public class ChessPiece implements ChessPieceInterface{
  
 private int pieceType;
 private int pieceTeam;
  
 public ChessPiece(int p, int t){
   pieceType = p;
   pieceTeam = t;
 }//ChessPiece
  
 /**
  * gets the piece type of a certain piece
  * @return int value of 1 - 6 representing the different pieces in chess
  */
 public int getType(){
   return this.pieceType;
 }//getType
 
 /**
  * gets the team of a certain piece
  * @return int value of 1 or 2 representing a team
  */
 public int getTeam(){
   return this.pieceTeam;
 }//getTeam
  
}//ChessPiece